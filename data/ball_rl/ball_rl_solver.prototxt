net: "data/ball_rl/ball_rl_train.prototxt"

# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.001
momentum: 0.9
weight_decay: 0.0005


# The learning rate policy
lr_policy: "step"
gamma: 0.1
power: 0.75

# Display every 100 iterations
display: 1

stepsize: 20000

# The maximum number of iterations
max_iter: 100000

# solver mode: CPU or GPU
solver_mode: CPU

snapshot: 0
