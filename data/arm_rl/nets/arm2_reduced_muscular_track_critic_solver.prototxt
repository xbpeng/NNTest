net: "data/arm_rl/nets/arm2_reduced_muscular_track_critic_net.prototxt"

# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.01
momentum: 0.9
weight_decay: 0.0005


# The learning rate policy
lr_policy: "fixed"
gamma: 0.1
power: 0.75

# Display every 100 iterations
display: 1

stepsize: 100000

# The maximum number of iterations
max_iter: 100000000

# solver mode: CPU or GPU
solver_mode: CPU

snapshot: 0
